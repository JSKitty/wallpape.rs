{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "withGlobalTauri": true,
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../dist",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Wallpape.rs",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "open": true
      },
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": []
      }
    },
    "bundle": {
      "active": true,
      "category": "Utility",
      "copyright": "JSKitty",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.wallpape.rs",
      "longDescription": "A simple GUI wallpaper manager written in Rust",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "A simple GUI wallpaper manager written in Rust",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 420,
        "resizable": true,
        "title": "Wallpape.rs",
        "width": 800
      }
    ]
  }
}